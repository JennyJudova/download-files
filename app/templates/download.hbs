<h1>Download Files</h1>
{{log @model}}
<div class="fileWrapper">
  <div class="topRow">
    <input type="checkbox" />
    <h3>{{selected-counter @model}}</h3>
    <h3>{{this.counter}}</h3>
    <h3>Download Selected</h3>
  </div>
  {{#each @model as |file|}}
  <File @file={{file}} @model={{@model}} />
  {{/each}}
</div>